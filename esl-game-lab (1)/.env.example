# Backend API URL
# In development, leave empty to use relative paths (proxied by Vite)
# In production, this will be set to the backend API URL automatically
VITE_API_URL=

# Admin Configuration
# Change this to a secure code of your choice
VITE_ADMIN_CODE=your_admin_code_here

# Firebase Configuration
# Get these values from your Firebase project settings
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id_here
